def ret(nums, che):
    ls = []
    num = nums[0]
    while len(nums) > 0:
        for i in nums:
            num = i
            nums.remove(i)
            for i in nums:
                if num + i == che:
                    ls.append(num)
                    ls.append(i)
    return ls
    





print(ret([3, 4, 3, 1], 6))
